#include<bits/stdc++.h>

using namespace std;

int main(){
    cout<<"Enter the string: ";
    string str;
    cin>>str;

    int len = str.size();
    int j = len-1;
    int flag = 0;

    for(int i = 0; i<=len/2; i++){
        if(str[i] != str[j])
            flag = 1;

        j--;
    }

    if(flag == 0)
        cout<<"Is Palindrome"<<endl;
    else{
        cout<<"Is not Pallindrome"<<endl;
    }
    

    return 0;
}
